output_dir          ./data/debruijn/
output_to_console   true

spades
{
  iterative_K         21 33 55
  paired_mode         true
  dataset             $cfg/test_dataset/input/test_dataset.info ; not used if run with bh
}

bh
{
  input_reads         $cfg/test_dataset/input/E.coli/is220/cropped/s_6.first1000_1.fastq.gz $cfg/test_dataset/input/E.coli/is220/cropped/s_6.first1000_2.fastq.gz
  output_dir          ./data/output/ECOLI_ALEX       ; optional
  max_iterations      4
  max_threads         20
  max_memory          50 ; in GB 
  gzip_output         true 
  single_cell         true
  reference           $cfg/test_dataset/input/E.coli/MG1655-K12.first1K.fasta.gz ; optional        
}
