; output options
write_seeds             true 
write_overlaped_paths   true
write_paths             true
write_contigs           true
write_real_paired_info  true
cluster_paired_info     false
write_graph             true
paired_info_file_prefix "paired_info_"

; seed selection
ss
{
    min_coverage   150
    glue_seeds     false
}

; extension selection
es
{
    use_weight_function_first  false
    weight_fun_threshold       0
    weight_threshold           0

    use_delta_first       false
    real_distance_dev     5
    etalon_distance_dev   2
}    

; loops removal
lr
{
    max_loops          5
    full_loop_removal  true
}

; stop criteria
sc
{
    all_seeds       true
    edge_coverage   0.8
    len_coverage    0.99
}

; filter options
fo
{
    remove_duplicates       true
    remove_subpaths         true
    remove_overlaps         false
}


;research options
research 
{
    research_mode            false
    detailed_output          false
    fiter_by_edge            true
    edge_length              46
    force_to_cycle           true 
    cycle_priority_edge      55
}
