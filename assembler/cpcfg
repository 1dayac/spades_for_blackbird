#!/bin/bash

cmd=cp
if [ "$1" = "-d" ] ; then
    cmd=diff
fi
$cmd src/debruijn/config.info.template                src/debruijn/config.info
$cmd src/debruijn/simplification.info.template        src/debruijn/simplification.info
$cmd src/debruijn/distance_estimation.info.template   src/debruijn/distance_estimation.info

